{
  "name": "@scip-sales/backoffice",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=--max-old-space-size=4096 nuxt",
    "build": "NODE_ENV=production nuxt build && NODE_ENV=production nuxt generate",
    "deploy": "yarn build && scp -r dist/* tri@thomasjacob.de:sites/virtual-hosts/vueground.thomasjacob.de/",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue,.ts,.gql --ignore-path .gitignore .",
    "lint:style": "stylelint --cache **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "typecheck": "tsc --noEmit",
    "serve-static": "yarn ws -d ./dist --spa index.html -p 3003"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@nuxtjs/composition-api": "^0.21.0",
    "core-js": "^3.9.1",
    "date-fns": "^2.20.3",
    "debounce": "^1.2.1",
    "file-saver": "^2.0.5",
    "nuxt": "^2.15.2",
    "nuxt-i18n": "^6.20.4",
    "vue-fragment": "^1.5.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@nuxt/types": "^2.15.2",
    "@nuxt/typescript-build": "^2.0.6",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/vuetify": "^1.11.3",
    "@sumcumo/eslint-config": "^6.1.5",
    "@sumcumo/jest-matchers": "^1.0.16",
    "@sumcumo/prettier-config": "^1.1.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/user-event": "^12.8.1",
    "@testing-library/vue": "^5.6.1",
    "@types/jest": "^26.0.20",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "esbuild-loader": "^2.9.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.5.0",
    "local-web-server": "^4.2.1",
    "speed-measure-webpack-plugin": "^1.4.2",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3",
    "vue-jest": "^3.0.4"
  }
}
